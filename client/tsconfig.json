{
	"compilerOptions": {
	  "noImplicitAny": true,
	  "module": "commonjs",
	  "target": "es6",
	  "allowJs": true,
	  "lib": ["es2017", "dom"],
	  "types": ["@citizenfx/client"],
	  "moduleResolution": "node",
	  "sourceMap": true,
	  "resolveJsonModule": true,
	  "esModuleInterop": true
	},
	"include": ["./**/*", "../typings"],
	"exclude": ["**/node_modules", "**/__tests__/*"]
  }
  